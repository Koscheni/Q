#include <locale.h>
#include <stdio.h>
#include <math.h>
#include <conio.h>
 
int main()
{
    int m, d, t, g;
    printf("Ваш пол: "); scanf("%u", &t);
    if (t<1 || t>31) printf("Такого дня нету!!");
    if (t = 1) printf("Женский");
    if (t = 2) printf("Мужской");
    if (t = 3) printf("Не определился");
    if (t = 4) printf("М");
    if (t = 5) printf("Ж");
    if (t = 6) printf("Нет");
}
